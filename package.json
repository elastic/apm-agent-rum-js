{
  "name": "elastic-apm-rum",
  "private": true,
  "version": "0.0.0-monorepo",
  "description": "Elastic APM RUM agent",
  "repository": {
    "type": "git",
    "url": "https://github.com/elastic/apm-agent-rum-js.git"
  },
  "scripts": {
    "version": "node scripts/prompt-release",
    "prerelease": "npm run clean",
    "release": "lerna publish && npm run github-release",
    "release-package": "node scripts/publish-package",
    "github-release": "node scripts/github-release",
    "build-docs": "PREVIEW=1 sh ./scripts/build_docs.sh apm-agent-rum-js ./docs ./build",
    "lint": "run-p --silent lint:*",
    "lint:js": "eslint . --ext .js,.jsx,.ts",
    "lint:license": "node scripts/license-checker",
    "test": "npm run build && lerna run test --scope=$SCOPE --no-prefix  --concurrency=1",
    "test:unit": "lerna run test:unit --scope=$SCOPE --no-prefix --concurrency=1",
    "build": "lerna run build",
    "bundlesize": "lerna run build:module && lerna run bundlesize",
    "ci:bundlesize": "bash ./scripts/ci-bundlesize.sh",
    "serve": "node ./dev-utils/run-script.js startTestServers",
    "coverage": "lerna run build:module && lerna run karma:coverage",
    "bench:dev": "lerna run build:module && node ./scripts/benchmarks.js",
    "package:snapshot": "lerna exec npm pack",
    "clean": "lerna exec -- rm -rf dist/",
    "ci:prepare-release": "node ./scripts/ci-prepare-release.js",
    "ci:pre-release": "node ./scripts/ci-pre-release.mjs",
    "ci:release": "node ./scripts/ci-release.mjs",
    "ci:post-release": "node ./scripts/ci-post-release.mjs"
  },
  "lint-staged": {
    "*.{js,jsx,ts}": [
      "eslint . --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "scope-enum": [
        2,
        "always",
        [
          "benchmarks",
          "deps",
          "dev",
          "jenkins",
          "karma",
          "release",
          "rum",
          "rum-angular",
          "rum-core",
          "rum-react",
          "rum-vue"
        ]
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "author": "",
  "license": "MIT",
  "homepage": "https://www.elastic.co/guide/en/apm/agent/rum-js/current/index.html",
