{
  "name": "elastic-apm-rum",
  "private": true,
  "version": "0.0.0-monorepo",
  "description": "Elastic APM RUM agent",
  "repository": {
    "type": "git",
    "url": "https://github.com/elastic/apm-agent-rum-js.git"
  },
  "scripts": {
    "version": "node scripts/prompt-release",
    "prerelease": "npm test && npm run clean",
    "release": "lerna publish && npm run github-release",
    "release-package": "node scripts/publish-package",
    "github-release": "node scripts/github-release",
    "build-docs": "PREVIEW=1 sh ./scripts/build_docs.sh apm-agent-rum-js ./docs ./build",
    "lint": "run-p --silent lint:*",
    "lint:js": "eslint . --ext .js,.jsx,.ts",
    "lint:license": "node scripts/license-checker",
    "test": "lerna run build && lerna run test --stream --scope=$SCOPE --no-prefix",
    "bundlesize": "lerna run build:module && lerna run bundlesize --stream",
    "serve": "node ./dev-utils/run-script.js startTestServers",
    "coverage": "lerna run build:module && lerna run karma:coverage --stream",
    "bench:dev": "lerna run build:module && node ./scripts/benchmarks.js",
    "clean": "npx lerna exec -- rm -rf dist/"
  },
  "lint-staged": {
    "*.{js,jsx,ts}": [
      "eslint . --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "",
  "license": "MIT",
  "homepage": "https://www.elastic.co/guide/en/apm/agent/rum-js/current/index.html",
