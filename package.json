{
  "name": "elastic-apm-rum",
  "private": true,
  "version": "0.0.0-monorepo",
  "description": "Elastic APM RUM agent",
  "repository": {
    "type": "git",
    "url": "https://github.com/elastic/apm-agent-rum-js.git"
  },
  "scripts": {
    "version": "node scripts/prompt-release",
    "prerelease": "npm test",
    "release": "lerna publish && npm run github-release",
    "release-package": "node scripts/publish-package",
    "github-release": "node scripts/github-release",
    "build-docs": "sh ./scripts/build_docs.sh apm-agent-rum-js ./docs ./build",
    "lint": "npm-run-all --silent -p lint:*",
    "lint:js": "eslint .",
    "lint:license": "node scripts/license-checker",
    "test": "lerna run build:main && lerna run test --stream --scope=$SCOPE --no-prefix",
    "bundlesize": "lerna run bundlesize --stream",
    "serve": "node ./dev-utils/run-script.js startTestServers",
    "karma": "karma start"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint . --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "",
  "license": "MIT",
  "homepage": "https://www.elastic.co/guide/en/apm/agent/js-base/current/index.html",
  "devDependencies": {
