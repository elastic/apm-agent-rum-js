{
  "name": "elastic-apm-js-base",
  "version": "2.1.0",
  "description": "",
  "main": "dist/bundles/elastic-apm-js-base.umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/elastic/apm-agent-js-base.git"
  },
  "scripts": {
    "prepare-patch": "sh ./node_modules/elastic-apm-js-core/dev-utils/prepare-release patch && npm run build && npm run push-release",
    "prepare-minor": "sh ./node_modules/elastic-apm-js-core/dev-utils/prepare-release minor && npm run build && npm run push-release",
    "prepare-major": "sh ./node_modules/elastic-apm-js-core/dev-utils/prepare-release major && npm run build && npm run push-release",
    "push-release": "git push origin master --follow-tags && npm publish",
    "build": "node node_modules/webpack/bin/webpack.js",
    "karma": "node node_modules/karma/bin/karma start",
    "karma-coverage": "node node_modules/karma/bin/karma start --coverage",
    "serve": "node ./scripts/run-script.js serveE2e ./ 8000",
    "build-docs": "sh ./scripts/build_docs.sh apm-agent-js-base ./docs ./build",
    "changelog": "node node_modules/conventional-changelog-cli/cli.js  -p angular -i CHANGELOG.md -s",
    "build-dev": "node node_modules/webpack/bin/webpack.js -w",
    "runUnitTests": "node ./scripts/run-script.js runUnitTests",
    "runNodeTests": "node ./scripts/run-script.js runNodeTests",
    "buildE2eBundles": "node ./scripts/run-script.js buildE2eBundles",
    "run-e2e": "node ./scripts/run-script.js runE2eTests",
    "test": "node node_modules/standard/bin/cmd.js src/**/* && npm run build && npm run buildE2eBundles && npm run runNodeTests && npm run runUnitTests && npm run run-e2e && npm run build-docs",
    "run-script": "node ./scripts/run-script.js",
    "format": "prettier-standard 'src/**/*.js'"
  },
  "files": [
    "src/**/*.js",
    "test/**/*.js",
    "dist"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "conventional-changelog-cli": "^1.3.22",
    "express": "^4.16.2",
    "http-server": "^0.10.0",
    "jasmine": "^2.8.0",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-failed-reporter": "^0.0.3",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-sauce-launcher": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.4",
    "prettier-standard": "^8.0.1",
    "puppeteer": "^1.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "sauce-connect-launcher": "^1.2.3",
    "selenium-standalone": "^6.15.2",
    "serve-index": "^1.9.1",
    "source-map": "^0.7.0",
    "standard": "^10.0.3",
    "string-replace-loader": "^1.3.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "wdio-jasmine-framework": "^0.3.2",
    "wdio-spec-reporter": "^0.1.3",
    "webdriverio": "^4.13.2",
    "webpack": "^3.5.6",
    "webpack-bundle-analyzer": "^2.9.2"
  },
  "dependencies": {
    "elastic-apm-js-core": "^2.1.0",
    "es6-promise": "^4.2.4"
  }
}
