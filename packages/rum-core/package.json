{
  "name": "@elastic/apm-rum-core",
  "version": "4.3.0",
  "description": "Elastic apm core",
  "license": "MIT",
  "main": "dist/lib/index.js",
  "module": "src/index.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elastic/apm-agent-rum-js.git",
    "directory": "packages/rum-core"
  },
  "scripts": {
    "prepublishOnly": "npm run build:main",
    "build:main": "npx babel src -d dist/lib --config-file ../../babel.node.js",
    "karma": "karma start",
    "karma:bench": "karma start karma.bench.conf.js",
    "karma:coverage": "karma start --coverage --singleRun",
    "test:unit": "node ../../dev-utils/run-script.js runUnitTests packages/rum-core true",
    "test": "npm run test:unit"
  },
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "error-stack-parser": "^1.3.5",
    "opentracing": "^0.14.3",
    "stack-generator": "^1.0.7",
    "uuid": "^3.1.0"
  }
}
