{
  "name": "elastic-apm-js-base",
  "version": "3.0.0",
  "description": "Elastic APM JavaScript agent",
  "main": "dist/bundles/elastic-apm-js-base.umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/elastic/apm-agent-js-base.git"
  },
  "scripts": {
    "prepare-patch": "sh ../../prepare-release patch && npm run build && npm run release",
    "prepare-minor": "sh ../../prepare-release minor && npm run build && npm run release",
    "prepare-major": "sh ../../prepare-release major && npm run build && npm run release",
    "release": "npm run push-tags && npm publish && npm run github-release",
    "push-tags": "git push origin master --follow-tags",
    "github-release": "node ./scripts/release-github.js",
    "build": "webpack",
    "build-dev": "webpack -w",
    "karma": "karma start",
    "karma-coverage": "karma start --coverage",
    "serve": "npm run script serveE2e ./ 8000",
    "changelog": "node node_modules/conventional-changelog-cli/cli.js  -p angular -i CHANGELOG.md -s",
    "runUnitTests": "npm run script runUnitTests",
    "runNodeTests": "npm run script runNodeTests",
    "buildE2eBundles": "npm run script buildE2eBundles",
    "run-e2e": "npm run script runE2eTests",
    "script": "node ./run-script.js",
    "test": "npm run build && npm run buildE2eBundles && npm run runNodeTests && npm run runUnitTests && npm run run-e2e"
  },
  "files": [
    "src/**/*.js",
    "dist"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {},
  "dependencies": {
    "elastic-apm-js-core": "^3.0.1",
    "es6-promise": "^4.2.4"
  }
}